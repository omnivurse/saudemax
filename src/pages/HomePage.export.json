        "className": "py-20 bg-gradient-to-r from-blue-700 to-green-600",
        "container": {
          "className": "max-w-4xl mx-auto text-center px-4 sm:px-6 lg:px-8",
          "content": {
            "className": "",
            "heading": {
              "className": "text-4xl lg:text-5xl font-bold text-white mb-6"
            },
            "description": {
              "className": "text-xl text-blue-100 mb-8"
            },
            "button": {
              "className": "size-lg variant-secondary"
            }
          }
        }
      }
    ]
  },
  "components": {
    "Button": {
      "variants": {
        "primary": "bg-blue-700 hover:bg-blue-800 text-white focus:ring-blue-500",
        "secondary": "bg-green-600 hover:bg-green-700 text-white focus:ring-green-500",
        "outline": "border-2 border-blue-700 text-blue-700 hover:bg-blue-700 hover:text-white focus:ring-blue-500",
        "ghost": "text-gray-700 hover:bg-gray-100 focus:ring-gray-500"
      },
      "sizes": {
        "sm": "px-4 py-2 text-sm",
        "md": "px-6 py-3 text-base",
        "lg": "px-8 py-4 text-lg"
      },
      "base": "inline-flex items-center justify-center font-medium rounded-lg transition-all duration-200 focus:outline-none focus:ring-2 focus:ring-offset-2 w-full"
    },
    "Card": {
      "base": "bg-white rounded-xl border border-gray-200",
      "hover": {
        "scale": 1.02,
        "shadow": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)"
      },
      "padding": {
        "sm": "p-4",
        "md": "p-6",
        "lg": "p-8"
      }
    },
    "motion": {
      "fadeIn": {
        "initial": { "opacity": 0, "y": 20 },
        "animate": { "opacity": 1, "y": 0 },
        "transition": { "duration": 0.6 }
      },
      "fadeInLeft": {
        "initial": { "opacity": 0, "x": -20 },
        "animate": { "opacity": 1, "x": 0 },
        "transition": { "duration": 0.6 }
      },
      "fadeInRight": {
        "initial": { "opacity": 0, "x": 20 },
        "animate": { "opacity": 1, "x": 0 },
        "transition": { "duration": 0.6 }
      },
      "stagger": {
        "children": {
          "initial": { "opacity": 0, "y": 30 },
          "animate": { "opacity": 1, "y": 0 },
          "transition": { "duration": 0.6, "delay": "index * 0.1" }
        }
      }
    }
  },
  "colorScheme": {
    "primary": {
      "light": "#3B82F6", // blue-500
      "main": "#1D4ED8", // blue-700
      "dark": "#1E40AF" // blue-800
    },
    "secondary": {
      "light": "#10B981", // green-500
      "main": "#047857", // green-700
      "dark": "#065F46" // green-800
    },
    "accent": {
      "light": "#8B5CF6", // purple-500
      "main": "#6D28D9", // purple-700
      "dark": "#5B21B6" // purple-800
    },
    "gradient": {
      "primary": "bg-gradient-to-br from-blue-900 via-blue-800 to-green-700",
      "secondary": "bg-gradient-to-r from-blue-700 to-green-600",
      "card": "bg-gradient-to-br from-blue-100 to-green-100"
    }
  },
  "typography": {
    "heading": {
      "h1": "text-5xl lg:text-7xl font-bold leading-tight",
      "h2": "text-4xl lg:text-5xl font-bold",
      "h3": "text-2xl font-bold",
      "h4": "text-xl font-semibold"
    },
    "body": {
      "large": "text-xl text-gray-600",
      "medium": "text-base text-gray-600",
      "small": "text-sm text-gray-500"
    }
  },
  "spacing": {
    "section": {
      "py": "py-20",
      "px": "px-4 sm:px-6 lg:px-8"
    },
    "container": "max-w-7xl mx-auto"
  },
  "breakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  }
}