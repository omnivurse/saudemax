{
  "sections": [
    {
      "id": "hero",
      "type": "hero",
      "style": {
        "background": "bg-gradient-to-br from-blue-900 via-blue-800 to-green-700",
        "minHeight": "min-h-screen",
        "position": "relative",
        "display": "flex",
        "alignItems": "items-center"
      },
      "overlay": {
        "enabled": true,
        "style": "bg-black/20 absolute inset-0"
      },
      "glassmorphism": {
        "enabled": true,
        "elements": [
          {
            "position": "absolute top-20 left-20 w-72 h-72 bg-white/10 rounded-full blur-3xl"
          },
          {
            "position": "absolute bottom-20 right-20 w-96 h-96 bg-green-500/10 rounded-full blur-3xl"
          }
        ]
      },
      "content": {
        "layout": "grid lg:grid-cols-2 gap-12 items-center",
        "leftColumn": {
          "animation": {
            "initial": { "opacity": 0, "x": -50 },
            "animate": { "opacity": 1, "x": 0 },
            "transition": { "duration": 0.8 }
          },
          "textColor": "text-white",
          "heading": {
            "size": "text-5xl lg:text-7xl",
            "weight": "font-bold",
            "lineHeight": "leading-tight",
            "marginBottom": "mb-6"
          },
          "subheading": {
            "size": "text-xl lg:text-2xl",
            "marginBottom": "mb-8",
            "color": "text-blue-100"
          },
          "ctaButtons": {
            "layout": "flex flex-col sm:flex-row gap-4",
            "primary": {
              "background": "bg-green-600 hover:bg-green-700",
              "textColor": "text-white",
              "padding": "px-8 py-4",
              "size": "text-lg",
              "weight": "font-medium",
              "borderRadius": "rounded-lg"
            },
            "secondary": {
              "background": "bg-white hover:bg-gray-50",
              "textColor": "text-blue-700",
              "padding": "px-8 py-4",
              "size": "text-lg",
              "weight": "font-medium",
              "borderRadius": "rounded-lg"
            }
          }
        },
        "rightColumn": {
          "animation": {
            "initial": { "opacity": 0, "x": 50 },
            "animate": { "opacity": 1, "x": 0 },
            "transition": { "duration": 0.8, "delay": 0.2 }
          },
          "card": {
            "background": "bg-white/10",
            "backdropBlur": "backdrop-blur-md",
            "borderRadius": "rounded-3xl",
            "padding": "p-8",
            "border": "border border-white/20"
          }
        }
      }
    },
    {
      "id": "stats",
      "type": "stats",
      "style": {
        "background": "bg-white",
        "padding": "py-16"
      },
      "content": {
        "container": "max-w-7xl mx-auto px-4 sm:px-6 lg:px-8",
        "grid": "grid grid-cols-2 lg:grid-cols-4 gap-8",
        "items": [
          {
            "value": "50,000+",
            "label": "Active Members",
            "animation": {
              "delay": 0.1
            }
          },
          {
            "value": "98%",
            "label": "Satisfaction Rate",
            "animation": {
              "delay": 0.2
            }
          },
          {
            "value": "$2.5B+",
            "label": "Medical Bills Shared",
            "animation": {
              "delay": 0.3
            }
          },
          {
            "value": "24/7",
            "label": "Support Available",
            "animation": {
              "delay": 0.4
            }
          }
        ]
      }
    },
    {
