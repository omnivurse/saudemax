{
  "meta": {
    "title": "SaudeMAX Landing Page Complete Design Export",
    "description": "Full design system export including layout, styling, components, and animations",
    "version": "1.0.0",
    "framework": "React + Tailwind CSS + Framer Motion"
  },
  "layout": {
    "container": "overflow-hidden",
