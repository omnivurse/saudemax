      {
        "id": "hero",
        "type": "hero",
        "className": "relative min-h-screen bg-gradient-to-br from-blue-900 via-blue-800 to-green-700 flex items-center",
        "overlay": {
          "enabled": true,
          "className": "absolute inset-0 bg-black/20"
        },
        "glassmorphism": {
          "enabled": true,
          "container": "absolute inset-0",
          "elements": [
            {
              "className": "absolute top-20 left-20 w-72 h-72 bg-white/10 rounded-full blur-3xl"
            },
            {
              "className": "absolute bottom-20 right-20 w-96 h-96 bg-green-500/10 rounded-full blur-3xl"
            }
          ]
        },
        "content": {
          "container": "relative max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-20",
          "layout": "grid lg:grid-cols-2 gap-12 items-center",
          "leftColumn": {
            "className": "text-white",
            "animation": {
              "initial": { "opacity": 0, "x": -50 },
              "animate": { "opacity": 1, "x": 0 },
              "transition": { "duration": 0.8 }
            },
            "heading": {
              "className": "text-5xl lg:text-7xl font-bold leading-tight mb-6",
              "content": "{{title}}"
            },
            "subheading": {
              "className": "text-xl lg:text-2xl mb-8 text-blue-100",
              "content": "{{subtitle}}"
            },
            "buttons": {
              "className": "flex flex-col sm:flex-row gap-4",
              "primary": {
                "className": "flex-1 inline-flex items-center justify-center px-8 py-4 text-lg font-medium text-white bg-green-600 hover:bg-green-700 rounded-lg transition-all duration-200 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-green-500",
                "content": "{{primaryButtonText}}",
                "link": "{{primaryButtonLink}}",
                "icon": "ArrowRight"
              },
              "secondary": {
                "className": "flex-1 inline-flex items-center justify-center px-8 py-4 text-lg font-medium text-blue-700 bg-white hover:bg-gray-50 rounded-lg transition-all duration-200 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-white",
                "content": "{{secondaryButtonText}}",
                "link": "{{secondaryButtonLink}}"
              }
            }
          },
          "rightColumn": {
            "className": "relative",
            "animation": {
              "initial": { "opacity": 0, "x": 50 },
              "animate": { "opacity": 1, "x": 0 },
              "transition": { "duration": 0.8, "delay": 0.2 }
            },
            "card": {
              "className": "bg-white/10 backdrop-blur-md rounded-3xl p-8 border border-white/20",
              "heading": {
                "className": "text-center mb-6",
                "title": {
                  "className": "text-2xl font-bold text-white mb-2",
                  "content": "{{cardTitle}}"
                },
                "subtitle": {
                  "className": "text-blue-100",
                  "content": "{{cardSubtitle}}"
                }
              },
              "steps": {
                "className": "space-y-4",
                "items": [
                  {
                    "className": "flex items-center space-x-3 text-white",
                    "icon": {
                      "className": "w-6 h-6 bg-green-500 rounded-full flex items-center justify-center",
                      "iconName": "Check"
                    },
                    "content": "{{step1}}"
