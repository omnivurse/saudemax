  "businessLogic": {
    "formHandling": {
      "validation": {
        "nameRequired": "Name is required",
        "emailRequired": "Email is required", 
        "emailInvalid": "Please enter a valid email address",
        "messageRequired": "Message is required"
      },
      "submitFlow": [
        "1. Validate all required fields",
        "2. Set isSubmitting to true",
        "3. Send POST request to form endpoint",
        "4. Handle success: Reset form, show success message",
        "5. Handle error: Show error message, keep form data",
        "6. Set isSubmitting to false"
      ],
      "apiIntegration": {
        "endpoint": "{{contactFormEndpoint}}",
        "method": "POST",
        "requestBody": {
          "name": "formData.name",
          "email": "formData.email", 
          "message": "formData.message"
        },
        "responseHandling": {
          "success": "response.ok === true",
          "error": "response.ok === false || network error"
        }
      }
    },
    
    "contactMethods": {
      "phone": {
        "display": "(561) 466‑9558",
        "href": "tel:+15614669558",
        "international": "+1 561 466 9558"
      },
      "email": {
        "display": "info@saude‑max.com",
        "href": "mailto:info@saude-max.com"
      },
      "address": {
        "line1": "SaudeMax Headquarters",
        "line2": "Florida, United States",
        "googleMapsQuery": "SaudeMax+Florida"
      },
      "hours": {
        "weekdays": "Monday - Friday: 8:00 AM - 6:00 PM EST",
        "saturday": "Saturday: 9:00 AM - 2:00 PM EST", 
        "sunday": "Sunday: Closed",
        "timezone": "EST"
      }
    }
  },
  
  "integrations": {
    "analytics": {
      "trackEvents": [
        "contact_form_view",
        "contact_form_submit", 
        "contact_form_success",
        "contact_form_error",
        "phone_click",
        "email_click",
        "quick_contact_call",
        "quick_contact_email"
      ]
    },
    
    "thirdPartyServices": {
      "googleMaps": {
        "embedCode": "{{googleMapsEmbedCode}}",
        "apiKey": "{{googleMapsApiKey}}",
        "center": "{{businessLatitude}}, {{businessLongitude}}"
