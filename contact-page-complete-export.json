  },
  
  "typography": {
    "headings": {
      "h1": "text-4xl lg:text-5xl font-bold",
      "h2": "text-2xl font-bold",
      "h3": "text-xl font-bold",
      "h4": "font-semibold"
    },
    "body": {
      "large": "text-xl",
      "medium": "text-base",
      "small": "text-sm"
    },
    "colors": {
      "heading": "text-gray-900",
      "body": "text-gray-600",
      "muted": "text-gray-500",
      "accent": "text-blue-700"
    }
  },
  
  "spacing": {
    "sections": {
      "default": "py-12",
      "large": "py-16",
      "small": "py-8"
    },
    "containers": {
      "default": "max-w-7xl mx-auto px-4 sm:px-6 lg:px-8",
      "narrow": "max-w-4xl mx-auto px-4 sm:px-6 lg:px-8"
    },
    "gaps": {
      "xs": "gap-2",
      "sm": "gap-3", 
      "md": "gap-4",
      "lg": "gap-6",
      "xl": "gap-12"
    }
  },
  
  "responsiveBreakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  },
  
